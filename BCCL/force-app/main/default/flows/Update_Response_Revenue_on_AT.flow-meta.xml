        <name>AccountTaggedwithKeyAccount</name>
        <dataType>Boolean</dataType>
        <expression>if(contains(UPPER(IF(ISBLANK({!$Record.rsp_Achievement__r.rsp_Account__r.Horizontal_Tagged__c}), &apos;&apos;, {!$Record.rsp_Achievement__r.rsp_Account__r.Horizontal_Tagged__c})), &apos;KEY ACCOUNT&apos;), true, false)</expression>
