    <formulas>
        <name>CollectedAmount</name>
        <dataType>Number</dataType>
        <expression>IF (
    {!$Record.rsp_Achievement__r.rsp_Order_Line_Item__r.Collected_Amount__c} * {!$Record.rsp_Achievement_Value__c} &lt; 0, 
    - 1, 
    1
) * 
{!$Record.rsp_Achievement__r.rsp_Order_Line_Item__r.Collected_Amount__c} * 
IF (
    {!$Record.rsp_Achievement__r.rsp_Order_Line_Item__r.TotalPrice} &lt;&gt; 0, (
        {!$Record.rsp_Achievement_Value__c} / 
        {!$Record.rsp_Achievement__r.rsp_Order_Line_Item__r.TotalPrice}
    ), 
    0
)</expression>
        <scale>2</scale>
    </formulas>
