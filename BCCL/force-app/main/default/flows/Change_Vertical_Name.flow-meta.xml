        <expression>Substitute({!VerticalName},&quot; &quot;,&quot;_&quot;)</expression>
    </formulas>
    <formulas>
        <description>Formula to Fetch Vertical Name within Role Name</description>
        <name>VerticalNamewithinRoleName</name>
        <dataType>String</dataType>
        <expression>LEFT(RIGHT({!RoleSobjL1.Name},LEN({!RoleSobjL1.Name}) - FIND(&quot;-&quot;,{!RoleSobjL1.Name},1)-1),FIND(&quot;-&quot;,RIGHT({!RoleSobjL1.Name},LEN({!RoleSobjL1.Name}) - FIND(&quot;-&quot;,{!RoleSobjL1.Name},1)-1),1)-2)</expression>
    </formulas>
