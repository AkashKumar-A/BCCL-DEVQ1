    <formulas>
        <name>QuoteReplaceVertical</name>
        <dataType>String</dataType>
        <expression>Substitute({!QuoteSobj.Vertical__c},{!PreviousName_Vertical},{!VerticalName})</expression>
    </formulas>
    <formulas>
        <name>SubstituteOldVDevName_PubGroup</name>
        <dataType>String</dataType>
        <expression>Substitute(substitute({!PubGroupSobjL1.DeveloperName},{!PreviousName_Vertical},{!VerticalName}),&quot; &quot;,&quot;_&quot;)</expression>
    </formulas>
    <formulas>
        <name>SubstituteOldVName_PubGroup</name>
