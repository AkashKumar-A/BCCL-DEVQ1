global class rsp_BatchTargetShareWithBVH implements Database.Batchable<sObject> , Schedulable
{
    global set<id> setRoleId = new set<id>();
    global set<id> setBVHRoleId = new set<id>(); 
    global string strEvent = 'DO_NOT_UPDATE';
    
    global boolean strSendEmail = false;
    
    
    global rsp_BatchTargetShareWithBVH(set<id> setRid,set<id> setBvhRid, string strEvnt, boolean strEmail)
    {
        if(setBvhRid != null && setBvhRid.size() > 0){
            setBVHRoleId = setBvhRid;
        }
        if(setRid != null && setRid.size() > 0)
            setRoleId = setRid; 
                
        if(!String.isBlank(strEvnt))
            strEvent = strEvnt;
        
        if(strEmail != null)
            strSendEmail = strEmail;
        
    }
    global void execute(SchedulableContext sc)
    {
        if(test.isrunningtest())
        dummymethod();
    }
    
    global Database.QueryLocator start(Database.BatchableContext BC)     
    {        
        string strQuery;
        if(setBVHRoleId != null && setBVHRoleId.size() > 0)
            return Database.getQueryLocator([select id, Parent_Role__c, Parent_Role__r.rsp_Current_User__c, Parent_Role__r.rsp_Current_User__r.isActive from rsp_Role__c where Parent_Role__c in: setBVHRoleId]); 
        else if(setRoleId != null && setRoleId.size() > 0)
            return Database.getQueryLocator([select id, Parent_Role__c, Parent_Role__r.rsp_Current_User__c, Parent_Role__r.rsp_Current_User__r.isActive from rsp_Role__c where id in: setRoleId]);
        else
            return Database.getQueryLocator([select id, Parent_Role__c, Parent_Role__r.rsp_Current_User__c, Parent_Role__r.rsp_Current_User__r.isActive from rsp_Role__c where rsp_Profile__c != null AND rsp_Profile__r.rsp_Profile_Initials__c = 'STM']);
    }
    
    global void execute(Database.BatchableContext BC, List<rsp_Role__c> lstRole) 
    {   
        if(test.isrunningtest()) 
        dummymethod();
        
        set<id> setRoleId = new set<id>();
        map<id, list<Id>> mapRoleToParentUser = new map<id, list<id>>();
        for(rsp_Role__c objRole : lstRole)
        {
            setRoleId.add(objRole.id);
            
            if(objRole.Parent_Role__c != null && objRole.Parent_Role__r.rsp_Current_User__c != null && objRole.Parent_Role__r.rsp_Current_User__r.isActive)
            {
                if(!mapRoleToParentUser.containskey(objRole.id))
                    mapRoleToParentUser.put(objRole.id, new list<id>());
                mapRoleToParentUser.get(objRole.id).add(objRole.Parent_Role__r.rsp_Current_User__c);
            }            
        }
        
        list<rsp_Target__c> lstTarget = [select id,rsp_Role__c,(select id, parentId, UserOrGroupId,RowCause from Shares where RowCause = 'Access_To_Parent_Role__c') from rsp_Target__c where rsp_Role__c in: setRoleId ];
        list<rsp_target__share> lstShareRecord = new list<rsp_target__share>();
        
        if(lstTarget != null && lstTarget.size() > 0)
        {
            for(rsp_target__c objTarget : lstTarget)
            {
                set<id> setUserId = new set<id>();
                
                for(rsp_target__share objShare : objTarget.Shares)
                {
                    setUserId.add(objShare.UserOrGroupId);
                }
                if(mapRoleToParentUser.containskey(objtarget.rsp_Role__c) && mapRoleToParentUser.get(objtarget.rsp_Role__c).size() > 0)
                {
                    for(id userId : mapRoleToParentUser.get(objtarget.rsp_Role__c))
                    {
                        if(!setUserId.contains(userId))
                        {
                            rsp_target__share objTargetShare = new rsp_Target__share();
                            objTargetShare.ParentId = objTarget.id;
                            objTargetShare.UserOrGroupId = userId;
                            objTargetShare.AccessLevel = 'Edit';
                            objTargetShare.RowCause = Schema.rsp_Target__share.RowCause.Access_To_Parent_Role__c; 
                            lstShareRecord.add(objTargetShare);
                        }
                    }                    
                }
            }
            system.debug('==== lstShareRecord ====='+lstShareRecord.size());
            system.debug('==== lstShareRecord ====='+lstShareRecord);
            
            if(strEvent == 'SHARE_RECORDS' && lstShareRecord.size() > 0)
                insert lstShareRecord;
        }
    }
   
    
    global void finish(Database.BatchableContext BC) 
    {        
        if(strSendEmail == true)
        {
            String str = 'Dear user'+'\n';
            str = str + 'Target Sharing Batch has been completed. '+'\n';
            
            list<string> lstToIds = System.Label.DEV_NOTIFY_BATCH_COMPLETION_STATUS.split(',');
            
            if(lstToIds.size() > 0)
            {
                Messaging.SingleEmailMessage semail = new Messaging.SingleEmailMessage();            
                semail.setToAddresses(lstToIds); 
                semail.setSubject('BCCL Production Opportunity Achievment attribute Sync batch Status update'); 
                semail.setPlainTextBody(str); 
                Messaging.sendEmail(new Messaging.SingleEmailMessage[] {semail});
            }            
        }
    }
    
